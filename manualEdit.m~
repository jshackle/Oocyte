for kk = 2:607
    fname = ['Pics3/border_bfalg_' int2str(kk) '.tif'];
    %fname = ['Manual/border_' int2str(kk)];
    info = imfinfo(fname);
    A = imread(fname, 'Info', info);
    A = double(A);
    disp(isCompleteCircle(A));
    imageh = UIEdit(A);
    pause
    B = imageh.CData;
    imwrite(B, ['Manual/' ['border_' int2str(kk)]], 'TIFF');
    close
end